<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body>
<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>
<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2021 - OrderBookWebApp
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/lib/jquery.inputmask.min.js"></script>
<script src="~/lib/knockout/knockout-3.5.1.js" asp-append-version="true"></script>
<script src="~/lib/knockout/knockout.mapping-latest.js" asp-append-version="true"></script>
<script src="~/lib/knockout/knockout.validation.min.js" asp-append-version="true"></script>
<script src="~/js/custom/helpers/pageLoader.js" asp-append-version="true"></script>
<script src="~/js/custom/extensions/knockoutExtensions.js" asp-append-version="true"></script>
<script src="~/js/custom/helpers/KnockoutPagingViewModel.js" asp-append-version="true"></script>
@RenderSection("Scripts", required: false)
</body>
</html>