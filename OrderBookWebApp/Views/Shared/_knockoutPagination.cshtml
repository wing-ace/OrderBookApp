<script type="text/html" id="Pagination-Knockout-Template">
    <nav>
        <ul class="pagination" data-bind="visible: NeedPaging">
            <li class="page-item" data-bind="css: { disabled: !FirstPageActive() }">
                <a class="page-link" data-bind="click: GoToFirst" aria-label="First">
                    <span aria-hidden="true">&laquo;</span>
                    <span class="sr-only">First</span>
                </a>
            </li>
            <li class="page-item" data-bind="css: { disabled: !PreviousPageActive() }">
                <a class="page-link" aria-label="Previous" data-bind="click: GoToPrevious">
                    <span aria-hidden="true">&lsaquo;</span>
                    <span class="sr-only">Previous</span>
                </a>
            </li>            
            <li class="page-item" data-bind="css: { disabled: !NextPageActive() }">
                <a class="page-link" aria-label="Next" data-bind="click: GoToNext">
                    <span aria-hidden="true">&rsaquo;</span>
                    <span class="sr-only">Next</span>
                </a>
            </li>
            <li class="page-item" data-bind="css: { disabled: !LastPageActive() }">
                <a class="page-link" data-bind="click: GoToLast" aria-label="Last">
                    <span aria-hidden="true">&raquo;</span>
                    <span class="sr-only">Last</span>
                </a>
            </li>
        </ul>
    </nav>
</script>

<script type="text/html" id="Pagination-Knockout-Template-Info">
    <!-- ko if: TotalCount() === 0 -->
    The table is empty.
    <!--/ko-->
    <!-- ko if: TotalCount() > 0 -->   
    Page
    <span data-bind="text: CurrentPage"></span> of
    <span data-bind="text: LastPage"></span>
    <!--/ko-->
</script>